  <div class="videotn">
    <img src="thumbnail.png" alt="" width="100%">
  </div>
  <!--Video Details------------------------------------------------------->
  <div class="viddetails">
    <!--Video Name and Date------------------------------------------------------->
    <div class="row">
      <p>
        <%=match.title%>
      </p>
      <br>
      <p>
        <%=match.createdAt%>
      </p>
    </div>
    <!--Contributors------------------------------------------------------->
    <div class="row">
      <div class="contrigrp">
        <button class="contribtn"><a href="#"><img src="profile.png" alt="profile img" width="30">
            FarhanLION</a></button>
        <button class="contribtn"><a href="#"><img src="profile.png" alt="profile img" width="30"> Julie
            Song</a></button>
        <button class="contribtn"><a href="#"><img src="profile.png" alt="profile img" width="30">
            Mavis</a></button>
      </div>
    </div>
    <!--Video Descriptions------------------------------------------------------->
    <div class="row">
      <p>
        <%=match.description%>
      </p>
    </div>
  </div>
</div>
