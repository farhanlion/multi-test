  <%- include('../partials/navbar'); %>

    <div class="bg-dark">

      <br>
      <br>

      <div class="universal-controls">
        <i id='pausebtn' class="fa fa-pause"></i>
        <i id='playbtn' class="fa fa-play"></i>
        <i id='stopbtn' class="fa fa-stop"></i>
      </div>

      <div class=" d-flex justify-content-center bg-dark">
        <!-- first box -->

        <div id="box" class="border border-light border-5">
          <p id="describe">Click to Upload Videos</p>
          <div id="upload">

            <label class="uploader" for="uploadinput">
              <!-- <h2>Upload</h2> -->
              <div class="btn btn-primary">Upload</div>
              <!-- <button type="button" class="btn btn-primary">Primary</button> -->
              <input id="uploadinput" class="global-cloudinary-fileupload" data-cloudinary-field="image"
                data-form-data="{&quot;upload_preset&quot;:&quot;dnqtrhtb&quot;}"
                data-url="https://api.cloudinary.com/v1_1/dvapwslkg/auto/upload" name="file" type="file">
            </label>
            <div class="progress-bar-holder">
              <div class="progress_bar" style="height: 10px;background-color: red;"></div>
            </div>



          </div>

        </div>
      </div>
