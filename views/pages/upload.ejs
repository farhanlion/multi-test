                <div class="upload btn btn-primary" id="uploadbtn2">Upload</div>
                <!-- <button type="button" class="btn btn-primary">Primary</button> -->
                <input id="uploadinput2" class="cloudinary-fileupload" data-cloudinary-field="image"
                  data-form-data="{&quot;upload_preset&quot;:&quot;dnqtrhtb&quot;}"
                  data-url="https://api.cloudinary.com/v1_1/dvapwslkg/auto/upload" name="file" type="file">
              </label>
              <div class="remove btn btn-primary" id="removebtn2">Remove</div>
            </div>
          </div>

          <div class="previewbox" id="box-3">
            <div class="preview border border-secondary border-3 " style="padding:10px 15px; border-radius: 15px;">
