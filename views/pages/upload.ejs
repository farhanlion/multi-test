            <div class="preview border border-secondary border-3 " style="padding:10px 15px; border-radius: 15px;">
              <div class="player">
                <video id="player1" controls fluid class="cld-video-player">
                </video>
              </div>
              <div class="slider" id="slider1"></div>
              <img src="./unmute.svg" width="40" class="muteToggleImg" id="mutebtn1" />
