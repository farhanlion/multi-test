                <video id="player5" controls fluid class="cld-video-player">
                </video>
              </div>
              <div class="slider" id="slider5"></div>
              <img src="./unmute.svg" width="40" class="muteToggleImg" id="mutebtn5" />
              <label class="uploader" for="uploadinput5">

                <div class="upload btn btn-primary" id="uploadbtn5">Upload</div>
                <!-- <button type="button" class="btn btn-primary">Primary</button> -->
                <input id="uploadinput5" class="cloudinary-fileupload" data-cloudinary-field="image"
