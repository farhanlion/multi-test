<%- include("header") %>

<div class="container-fluid px-0 py-0" style="background-color: #212529; width:100%" controls id="viewer">

    <div class="row px-0 py-0 mx-auto" style="width:100%" id="row1">
        <% for(let i=1; i<= 2; i++) { %>
        <% iStr = "player" + i.toString() %>
        <div class="col-xl-6 col-lg-12 px-0">
            <video id=<%=iStr%>
                controls
                fluid  
                class="cld-video-player">
            </video>
        </div>
        <%}%>
    </div>
    
    <div class="row px-0 py-0 mx-auto" style="width:100%" id="row2">
        <% for(let i=3; i<= 5; i++) { %>
        <% iStr = "player" + i.toString() %>
        <div class="col-xl-4 col-lg-12 px-0">
            <video id=<%=iStr%>
                controls 
                fluid 
                class="cld-video-player">
            </video>
        </div>

        <%}%>
    </div>

    <div class="text-center m-0 p-0">
        <button class=" btn btn-default" id="mute-toggle-btn"><img src="mute.svg" alt="" width="50" id="muteToggleImg"></button>
        <button class="btn btn-default" id="fback-btn"> <img src= "./seekbackwards.svg" width="50" id="seekBackImg"/> </button>
        <button class=" btn btn-default" id="play-btn"><img src="./play.svg" alt="" width="50" id="playToggleImg"></button>
        <button class="btn btn-default" id="ffwd-btn"> <img src= "./seekforward.svg" width="50" id="seekFwdImg"/> </button>
        <div class=" btn btn-default" id="reset-btn"><img src="./sync.svg" alt="" width="50" id="syncImg"></div>
    </div>
</div>

<% if(numOfVideos==6) { %>

